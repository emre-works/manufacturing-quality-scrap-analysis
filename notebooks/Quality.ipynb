   "source": [
    "line_summary = (\n",
    "    df.groupby([\"line\", \"date\", \"shift\"])\n",
    "      .agg({\n",
    "          \"produced_qty\": \"first\",\n",
    "          \"defect_qty\": \"sum\"\n",
    "      })\n",
    "      .reset_index()\n",
    ")\n",
    "\n",
    "#Scrap rate formula\n",
    "line_summary[\"scrap_rate\"] = line_summary[\"defect_qty\"] / line_summary[\"produced_qty\"]\n",
    "\n",
    "#Scrap rate unit update (%) >> changing dtype NOK\n",
    "# line_summary[\"scrap_rate\"] = (line_summary[\"scrap_rate\"] * 100).round(2).astype(str) + \"%\"\n",
    "\n",
    "#Scrap rate unit update (%) >> same dtype\n",
    "pd.options.display.float_format = '{:.2%}'.format\n",
    "\n",
    "line_summary\n"
   ]
  },
